{
  "name": "rusticola",
  "version": "0.1.1",
  "description": "A 11ty website by Woodcox.",
  "main": "docs/index.html",
  "scripts": {
    "clean": "node config/clean/clean.js",
    "dev:sass": "sass  --no-source-map --watch src/sass:./_tmp",
    "dev:eleventy": "ELEVENTY_ENV=dev npx @11ty/eleventy-dev-server --input=docs",
    "build:sass": "sass  --no-source-map src/sass:./_tmp",
    "build:eleventy": "npx @11ty/eleventy",
    "build:css": "BROWSERSLIST_CONFIG='./package.json' lightningcss ./_tmp/*.css -o ./_tmp/*.css",
    "minify:css": "npm run build:css -- --minify",
    "start": "NODE_ENV=dev npm-run-all --parallel dev:**",
    "cloud": "NODE_ENV=dev ELEVENTY_ENV=dev npm-run-all clean build:sass build:eleventy",
    "build": "NODE_ENV=dev npm-run-all clean build:sass build:css 'ELEVENTY_ENV=dev build:eleventy -- --pathprefix=rusticola'",
    "minify": "NODE_ENV=production ELEVENTY_ENV=prod npm-run-all clean build:sass minify:css 'build:eleventy -- --pathprefix=rusticola'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woodcock3/11ty-solid-start.git"
  },
  "author": "woodcox",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.25",
    "@11ty/eleventy-plugin-webc": "^0.7.0",
    "@11ty/is-land": "^3.0.0",
    "@hyrious/esbuild-plugin-http": "^0.1.1",
    "esbuild": "^0.15.18",
    "esbuild-plugin-manifest": "^0.5.0",
    "esbuild-plugin-solid": "^0.4.2",
    "esbuild-svelte": "^0.7.3",
    "glob-all": "^3.3.1",
    "lightningcss-cli": "^1.18.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "solid-js": "^1.6.8",
    "svelte": "^3.55.0"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
