{
  "name": "11ty-solid-start",
  "version": "0.0.1",
  "description": "Nothing but a base HTML5 template and the setup to compile your Solid-js app alongside 11ty.",
  "main": "index.html",
  "scripts": {
    "dev:sass": "sass  --no-source-map --watch src/sass:./_tmp",
    "dev:eleventy": "ELEVENTY_ENV=dev npx @11ty/eleventy --serve",
    "build:sass": "sass  --no-source-map src/sass:./_tmp",
    "build:eleventy": "ELEVENTY_ENV=dev npx @11ty/eleventy --pathprefix=11ty-solid-start",
    "minify:eleventy": "ELEVENTY_ENV=prod npx @11ty/eleventy --pathprefix=bonsai",
    "postbuild": "lightningcss --minify --targets '> 0.25%, not IE 11' src/css/*.css -o public/css/*.css",
    "start": "npm-run-all --parallel dev:*",
    "build": "NODE_ENV=dev npm-run-all build:sass build:eleventy",
    "minify": "NODE_ENV=production npm-run-all build:sass minify:eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woodcock3/11ty-solid-start.git"
  },
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "esbuild": "^0.15.10",
    "esbuild-plugin-solid-js": "^1.0.1",
    "glob-all": "^3.3.0",
    "lightningcss-cli": "^1.16.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0",
    "solid-js": "^1.5.7"
  },
  "browserslist": [
    "> 0.25%",
    "not IE 11"
  ]
}
